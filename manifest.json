{
  "name": "dark-mode-rises",
  "description": "Lightweight chrome extension for automatically enabling/disabling dark mode on popular websites.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*", 
        "*://*.youtube.com/*", 
        "*://*.wikiwand.com/*",
        "*://*.reddit.com/*",
        "http://localhost:1313/"
      ],
      "js": ["main.js"]
    }
  ],
  "permissions": ["geolocation"]
}
