{
  "name": "dark-mode-rises",
  "description": "Chrome extension for automatically toggling dark mode on various websites.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*",
        "*://*.youtube.com/*",
        "*://*.wikiwand.com/*",
        "*://*.reddit.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "permissions": [
    "geolocation"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
